{
    "manifestVersion": 1,
    "id": "cleansourceext",
    "name": "Clean Sources Directory",
    "version": "0.8.0",
    "publisher": "TomAustin",
    "repository": {
        "type": "git",
        "url": "https://github.com/tomaustin700/AzureDevopsCleanSourcesExtension"
    },
    "content": {
        "details": {
            "path": "README.MD"
        },
        "license": {
            "path": "LICENCE.md"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/tomaustin700/AzureDevopsCleanSourcesExtension/issues"
        },
        "license": {
            "uri": "https://github.com/tomaustin700/AzureDevopsCleanSourcesExtension/blob/main/LICENSE.md"
        }
    },
    "tags": [
        "Azure Pipelines"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Removes all files and folders from the Sources directory.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "cleansource-ta",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "galleryFlags": [
        "Public"
    ]
}